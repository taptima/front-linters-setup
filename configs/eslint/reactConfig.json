{
  "extends": ["airbnb", "prettier", "./taptimaBaseConfig", "prettier/react"],
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": "off",
    "react/state-in-constructor": ["error", "never"],
    "react/jsx-props-no-spreading": [
      "error", {
      "html": "enforce",
      "custom": "ignore",
      "explicitSpread": "enforce",
      "exceptions": []
    }]
  }
}
